@model IEnumerable<Consume.Models.Team>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TeamId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TeamName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TeamId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TeamName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { TeamId = item.TeamId, teamname = item.TeamName, prod = item })
                @Html.ActionLink("Details", "Details", new { TeamId = item.TeamId }) |
                @Html.ActionLink("Delete", "Delete", new { TeamId = item.TeamId }) |
</tr>
}
    </tbody>
</table>
